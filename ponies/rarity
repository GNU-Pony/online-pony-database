(name (en "Rarity"))
(nickname Rare)
(kind pegasus)
(group mare)
(colour (coat white)
	(mane (. ubdigue purple lilac))
	(eye (. azure blue))
	(eyeshadow blue))
(cutie diamond)
(appearance (1 (focus 1..11 14 16..26)
	       (background 13))
	    (2 (focus 1..3 5..11 13..16 19..21 23..26)
	       (background 18))
	    (3 (focus 1..3 5..13)
	       (background 4))
(transformation ((flutter unicorn) (during 1 16))
