(name (en "Twilight Sparkle"))
(nickname Twilight Twi Twily)
(kind (1 unicode) ((3 13 end) (unicode pegasus)))
(group mare)
(colour (coat purple)
	(mane (purple blue violet pink))
	(eye (. violet purple)))
(cutie star)
(appearance (1 (focus 1..26))
	    (2 (focus 1..4 5..11 13..18 20..26))
	    (3 (focus 1..3 5 7 9..13)
	       (background 4)))
(transformation ((unicorn pegasus) (from 3 13 end)))
